{
  "name": "network-dashboard",
  "version": "1.0.0",
  "proxy": "http://127.0.0.1:5000",
  "dependencies": {
    "axios": "^1.4.0",
    "lucide-react": "^0.546.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.4",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
     "dev:api": "cd ../backend && node setup_backend.js",
  "dev:all": "cross-env PYTHONUNBUFFERED=1 concurrently -n \"API,UI\" -c \"green,blue\" \"npm run dev:api\" \"npm start\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "tailwindcss": "^3.4.18"
  }
}
